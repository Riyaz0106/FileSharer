 <!-- Modal -->
<div class="modal fade" id="dialogBoxmodel" style="color:grey" tabindex="-1" role="dialog" aria-labelledby="dialogBoxmodelTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 style="color:black" class="modal-title" id="dialogBoxmodelTitle">Dialog Box</h5>
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="container-fluid">
                    <p style="font-size: large;"></p>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">No</button>
                <form id="dialogForm" method="post" action='#'>
                    <input id="dialogFormInput" type="hidden" name="_method" value="#">
                    <button type="submit" class="btn btn-success">Yes</button>
                </form>
            </div>
        </div>
    </div>
</div>